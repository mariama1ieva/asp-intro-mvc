@model List<UserVM>

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" />

<table class="table">
    <h1>Users</h1>
    <tr>
        <th>FullName</th>
        <th>Email</th>
        <th>Address</th>
        <th>Age</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.FullName</td>
            <td>@item.Email</td>
            <td>@item.Address</td>
            <td>@item.Age</td>
        </tr>
    }
</table>




    
    

   