@model List<Product>

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" />

<table class="table">
    <h1>Products</h1>
    <tr>
        <th>Name</th>
        <th>Price</th>
        <th>Count</th>
        <th>Description</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Price</td>
            <td>@item.Count</td>
            <td>@item.Description</td>
        </tr>
    }
</table>