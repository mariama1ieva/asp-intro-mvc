@model List<BookVM>

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" />

<table class="table">
    <h1>Books</h1>
    <tr>
        <th>Name</th>
      
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
         
        </tr>
    }
</table>